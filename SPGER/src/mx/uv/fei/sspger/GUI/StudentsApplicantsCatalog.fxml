            </Pane>
            <HBox alignment="CENTER" prefHeight="97.0" prefWidth="800.0">
               <children>
                  <Button fx:id="btnAcept" mnemonicParsing="false" onMouseClicked="#aceptStudentsProject" prefHeight="25.0" prefWidth="83.0" text="Aceptar">
