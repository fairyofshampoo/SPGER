      </Pane>
      <VBox alignment="TOP_CENTER" layoutY="38.0" prefHeight="561.0" prefWidth="800.0">
         <children>
            <VBox alignment="CENTER" prefHeight="124.0" prefWidth="800.0">
               <children>
                  <Pane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="lblProjectTitle" layoutX="176.0" layoutY="33.0" prefHeight="27.0" prefWidth="448.0" wrapText="true">
                           <font>
                              <Font name="System Bold" size="18.0" />
